{
  "name": "react-generator",
  "version": "1.0.16",
  "description": "A React component container and test generation library",
  "repository": {
    "type": "git",
    "url": "git://github.com/wednesday-solutions/test-generator.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "author": "Wednesday Solutions",
  "license": "MIT",
  "scripts": {
    "gt": "plop --plopfile generators/existing/index.js",
    "gtcom": "plop --plopfile generators/existing/index.js component",
    "gtcon": "plop --plopfile generators/existing/index.js container",
    "gtf": "plop -f --plopfile generators/existing/index.js",
    "gtcomf": "plop -f --plopfile generators/existing/index.js component",
    "gtconf": "plop -f --plopfile generators/existing/index.js container",
    "g": "plop --plopfile generators/new/index.js",
    "gcom": "plop --plopfile generators/new/index.js component",
    "gcon": "plop --plopfile generators/new/index.js container",
    "gf": "plop -f --plopfile generators/new/index.js",
    "gcomf": "plop -f --plopfile generators/new/index.js component",
    "gconf": "plop -f --plopfile generators/new/index.js container",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "#npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.3",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/polyfill": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "circular-dependency-plugin": "5.0.2",
    "coveralls": "3.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-plugin-redux-saga": "1.0.0",
    "express": "4.16.4",
    "jest-cli": "24.7.1",
    "jest-dom": "3.1.3",
    "jest-styled-components": "6.3.1",
    "lint-staged": "8.1.5",
    "minimist": "1.2.0",
    "ngrok": "3.1.1",
    "node-plop": "0.18.0",
    "plop": "^2.4.0",
    "pre-commit": "1.2.2",
    "prettier": "1.17.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-intl": "2.8.0",
    "react-redux": "7.0.2",
    "react-router-dom": "5.0.0",
    "react-testing-library": "6.1.2",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0",
    "sanitize.css": "8.0.0",
    "shelljs": "^0.8.3",
    "styled-components": "4.2.0",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.6.0"
  },
  "@babel/cli": "7.4.3",
  "bin": {
    "react-generate": "bin/react-generate.js",
    "react-generate-all": "bin/react-generate-all.js"
  }
}
